<!DOCTYPE html>
<html lang="en" class="no-js">
<head>
    <meta charset="UTF-8">
    <title>新闻首页</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width">
    <meta name="renderer" content="webkit|ie-comp|ie-stand">
</head>
<body>

<div>
    <h1>$!{content}</h1>
</div>
<div>
    <div id="myView"></div>
</div>

<script src="js/jquery/jquery-3.2.0.min.js"></script>
<script>
    function initView() {
        $.getJSON("/$!{web_project}/getView", {id : 1}, function(result){
            $("#myView").text(result.view + ", " + result.id);
        });
    }
    initView();
</script>
</body>
</html>